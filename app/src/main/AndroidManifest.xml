<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vyaches.auto">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Vehicles.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Vehicles.AddVehicleActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Vehicles.VehicleActivity"
            android:label="@string/title_activity_vehicle"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Vehicles.ListVehicleActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Vehicles.ListInfoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Templates.TemplateActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Templates.AddTemplateActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".Templates.TemplateInfo" />
        <activity android:name=".Replacements.ReplacementActivity"></activity>
    </application>

</manifest>